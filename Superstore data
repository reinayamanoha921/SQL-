SELECT item_name
FROM superstore 
GROUP BY price;
SELECT SUM(price)
FROM superstore; 
SELECT AVG(price)
FROM superstore; 
SELECT MIN(price)
FROM superstore; 
SELECT MAX(price)
FROM superstore; 
SELECT COUNT(price)
FROM superstore;  


SELECT stock_quantity, item_name
FROM superstore 
GROUP BY item_name;

SELECT AVG(price), category
FROM superstore
WHERE category = "Kitchen Supplies"

